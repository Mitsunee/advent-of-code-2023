{
  "name": "@mitsunee/advent-of-code-2023",
  "version": "0.0.0",
  "description": "My solutions for Advent of Code 2023 written in TypeScript",
  "author": "Mitsunee",
  "repository": "mitsunee/advent-of-code-2023",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "lint": "eslint .",
    "lint:strict": "eslint --max-warnings 0 .",
    "typecheck": "tsc",
    "test": "uvu -r esbuild-register",
    "test:dir": "uvu -r esbuild-register --ignore '^(?!.*\\.test\\.ts).*\\.ts'"
  },
  "nano-staged": {
    "**/*.{js,cjs,mjs,ts,mts,cts,json,md,yaml}": [
      "prettier -w"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "esbuild": "^0.19.8",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.54.0",
    "eslint-config-foxkit": "3.0.3",
    "eslint-config-prettier": "^9.0.0",
    "nano-staged": "^0.8.0",
    "prettier": "^3.1.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "~5.2.0",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^11.1.0",
    "@foxkit/node-util": "^0.6.0",
    "@foxkit/util": "^0.6.2",
    "commander": "^11.1.0",
    "tsx": "^4.6.2"
  }
}
